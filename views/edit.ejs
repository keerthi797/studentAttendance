<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Student Collection</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
  <h2>Edit</h2>
  <% students.forEach(function(details) { %>
    
    <div class="container"><br>
      
          
      <form action="/edit/<%= details._id %>" method="POST"> 
          <div class="form-group">
              <label for="Full Name">User Name:</label>
              <input type="text" class="form-control" name="name" id="username" value="<%= details.username %>">
          </div>
          <div class="form-group">
            <label for="Full Name"> Name:</label>
            <input type="text" class="form-control" name="name" id="name" value="<%= details.name %>">
        </div>
           <div class="form-group">
              <label for="Class">Class :</label>
              <input type="text" class="form-control" name="stclass" id="stclass" value="<%= details.stclass %>">
          </div>
           <div class="form-group">
              <label for="Section">Section :</label>
              <input type="text" class="form-control" name="section" id="section" value="<%= details.section %>">
          </div><br>
      
            <div class="col-12">
          <!-- <button type="submit" class="btn btn-primary">Submit</button> -->
          <button class="btn btn-primary" style="background: white;"><a href="/show" >List All</a></button>
          <button type="submit" class="btn btn-primary">Submit</button>
        </div>
        </form>
      </div>
  <% }) %></body>
</html>